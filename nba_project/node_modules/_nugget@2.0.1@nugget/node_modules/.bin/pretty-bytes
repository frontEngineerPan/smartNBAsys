#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_pretty-bytes@1.0.4@pretty-bytes/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_pretty-bytes@1.0.4@pretty-bytes/cli.js" "$@"
  ret=$?
fi
exit $ret
